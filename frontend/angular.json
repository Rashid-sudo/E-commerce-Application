{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "projects": {
    "frontend": {
      "projectType": "application",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json"
          }
        }
      }
    }
  },
  "defaultProject": "frontend"
}